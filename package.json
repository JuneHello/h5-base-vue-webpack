{
  "name": "h5-vue3-vite-vuecli5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite --mode development",
    "serve": "vue-cli-service serve",
    "build:dev": "vue-tsc && vue-cli-service build --mode development",
    "build:test": "vue-tsc && vue-cli-service build --mode test",
    "build:pre": "vue-tsc && vue-cli-service build --mode pre",
    "build:prd": "vue-tsc && vue-cli-service build --mode production",
    "build:no-cache": "pnpm clean:cache && pnpm run build:prd",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "vue-cli-service lint",
    "lint:lint-staged": "lint-staged",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "core-js": "^3.8.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.12.0",
    "vant": "^4.8.3",
    "vconsole": "^3.15.1",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.20.7",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/node": "^20.8.9",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vant/auto-import-resolver": "^1.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-svg-sprites": "^1.2.5",
    "svg-sprite-loader": "^6.0.11",
    "sass": "^1.69.5",
    "sass-loader": "^12.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-less": "^2.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-less": "^2.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~4.5.5",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.0.3",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html-template": "^1.1.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.8.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
